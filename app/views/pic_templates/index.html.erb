<h1>All Photos</h1>

<a href="/photos/new">Add a new photo</a>

<% @list_of_photos.each do |a_photo|%>

<div class = "thumbnail">

<img src="<%= a_photo.source %>" >

<a class="btn btn-secondary btn-block" href="/photos/<%=a_photo.id%>">Show Details</a>
<a class="btn btn-secondary btn-block" href="/photos/<%=a_photo.id%>/edit" role="button">Edit</a>
<a class="btn btn-secondary btn-block" href="/delete_photo/<%=a_photo.id %>" role="button">Delete</a>

</div>

  <% end %>
